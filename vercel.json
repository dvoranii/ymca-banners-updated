{
  "rewrites": [
    {
      "source": "/:brand/:size/:city/assets/(.*)",
      "destination": "/:brand/:size/:city/assets/$1"
    },
    {
      "source": "/:brand/:size/:city/",
      "destination": "/:brand/:size/:city/index.html"
    },
    {
      "source": "/:brand/:size/:city",
      "destination": "/:brand/:size/:city/index.html"
    }
  ]
}
