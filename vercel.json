{
  "rewrites": [
    {
      "source": "/:brand/:size/:city/",
      "destination": "/:brand/:size/:city/index.html"
    },
    {
      "source": "/:brand/:size/:city",
      "destination": "/:brand/:size/:city/index.html"
    },
    {
      "source": "/:brand/:size/:city/assets/:filename",
      "destination": "/:brand/:size/:city/assets/:filename"
    },
    {
      "source": "/:brand/:size/assets/:filename",
      "destination": "/:brand/:size/assets/:filename"
    }
  ]
}
